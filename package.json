{
  "name": "bifrost-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run generate:llm-data && next build",
    "start": "next start",
    "lint": "next lint",
    "email": "email dev -d src/emails",
    "pages:build": "npm run generate:llm-data && npx @cloudflare/next-on-pages",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --kv=BIFROST_KV --compatibility-date=2024-09-23 --compatibility-flag=nodejs_compat_v2 --compatibility-flag=nodejs_compat_populate_process_env",
    "dev:cf": "npm run pages:build && npm run pages:dev",
    "generate:llm-data": "npx -y tsx scripts/generate-llm-data.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.983.0",
