  "devDependencies": {
    "@cloudflare/next-on-pages": "1.13.16",
    "@eslint/eslintrc": "3.3.3",
    "@react-email/preview-server": "5.0.6",
    "@tailwindcss/postcss": "4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "24.10.2",
    "@types/react": "19.2.7",
